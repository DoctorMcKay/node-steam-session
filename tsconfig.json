{
	"compilerOptions": {
		"lib": ["es2020"],
		"target": "es2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"inlineSourceMap": true,
		"skipLibCheck": true,
		"declaration": true,
		"declarationMap": true
	},
	"include": ["src"]
}
